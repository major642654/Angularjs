
<web-app>
  <display-name>Archetype Created Web Application</display-name>
   <servlet>
  	<servlet-name>loginController</servlet-name>
  	<servlet-class>com.java.controller.LoginController</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>loginController</servlet-name>
  	<url-pattern>/LoginController</url-pattern>
  </servlet-mapping>
  <servlet>
  	<servlet-name>registController</servlet-name>
  	<servlet-class>com.java.controller.RegistController</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>registController</servlet-name>
  	<url-pattern>/RegistController</url-pattern>
  </servlet-mapping>
  
  <servlet>
  	<servlet-name>IndexController</servlet-name>
  	<servlet-class>com.java.controller.IndexController</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>IndexController</servlet-name>
  	<url-pattern>/IndexController</url-pattern>
  </servlet-mapping>
  <servlet>
  	<servlet-name>UpdateController</servlet-name>
  	<servlet-class>com.java.controller.UpdateController</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>UpdateController</servlet-name>
  	<url-pattern>/UpdateController</url-pattern>
  </servlet-mapping>
  
  <servlet>
  	<servlet-name>DeleteController</servlet-name>
  	<servlet-class>com.java.controller.DeleteController</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>DeleteController</servlet-name>
  	<url-pattern>/DeleteController</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>login.html</welcome-file>
  </welcome-file-list>
</web-app>
